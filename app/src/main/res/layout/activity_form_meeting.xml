<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.meeting_form.FormMeetingActivity">

    <EditText
        android:id="@+id/item_form_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/form_date"
        android:inputType="date"
        tools:ignore="Autofill,MissingConstraints" />

    <EditText
        android:id="@+id/item_form_subject"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:hint="@string/form_subject"
        android:inputType="text"
        app:layout_constraintTop_toBottomOf="@+id/item_form_date"
        tools:ignore="Autofill,MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>